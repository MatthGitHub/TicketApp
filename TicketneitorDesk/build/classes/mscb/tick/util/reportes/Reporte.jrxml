	<parameter name="id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT id_ticket, em.nombre,em.apellido,a.nombre AS asunto, nombre_asuntoS AS servicio, fecha, SUBSTRING(CAST(hora AS CHAR),12,12) AS hora, e.nombre AS estado, respuesta,observacion
FROM tickets t
JOIN usuarios u
ON u.id_usuario = t.usuario_receptor
JOIN empleados em
ON em.id_empleado = u.fk_empleado
JOIN servicios s
ON t.asunto = s.id_asuntoS
JOIN asuntos a
ON a.id_asuntoP = s.pertenece
JOIN estados e
ON e.id_estado = t.fk_estado
WHERE id_ticket=$P{id}]]>
	</queryString>
