	<parameter name="id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT MAX(id_historial),id_ticket, a.nombre AS asunto, nombre_asuntoS AS servicio, t.fecha, SUBSTRING(CAST(ht.hora AS CHAR),12,12) AS hora, e.nombre AS estado, observacion,patrimonio
FROM tickets t
JOIN historial_tickets ht 
ON t.id_ticket = ht.fk_ticket
JOIN servicios s
ON t.servicio = s.id_asuntoS
JOIN asuntos a
ON a.id_asuntoP = s.pertenece
JOIN estados e
ON e.id_estado = ht.fk_estado
WHERE id_ticket=$P{id}]]>
	</queryString>
